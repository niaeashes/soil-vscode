{
  "name": "soil",
  "displayName": "soil Schema",
  "version": "0.0.1",
  "repository": "git@github.com:niaeashes/soil.git",
  "author": "niaeashes <nia.eashes@gmail.com>",
  "license": "MIT",
  "main": "./extension.js",
  "dependencies": {
    "chalk": "^5.0.1",
    "soil-schema": "^0.0.1",
    "vscode-languageclient": "^8.0.0"
  },
  "devDependencies": {
    "ava": "^4.3.1"
  },
  "scripts": {
    "test": "ava"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "soil",
        "extensions": [
          ".soil"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "soil",
        "scopeName": "source.soil",
        "path": "./syntaxes/soil.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:soil"
  ]
}
